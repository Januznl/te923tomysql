#!/usr/bin/env php
<?php

require __DIR__.'/../vendor/autoload.php';

use TE923ToMysql\Command;
use Symfony\Component\Console\Application;
use Symfony\Component\Yaml\Yaml;

$config = Yaml::parse(__DIR__.'/../config/config.yml');
var_dump($config);

$application = new Application('TE923ToMysql', '0.1-dev');
$application->add(new Command\HelloCommand());
$application->add(new Command\DoctrineCommand($application));
$application->run();
